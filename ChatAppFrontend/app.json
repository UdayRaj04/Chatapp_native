{
  "expo": {
    "jsEngine": "jsc",
    "name": "Seamless Chat",
    "slug": "chat-app-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.chatappfrontend"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/logos.png"
    }
  }
}
